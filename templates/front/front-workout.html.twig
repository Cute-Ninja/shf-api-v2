{% trans_default_domain 'front' %}

{% extends 'front/front-base.html.twig' %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('build/js/front-workout.js') }}"></script>
{% endblock %}

{% block title %}{{ parent() }}{{ 'front.workouts.title'|trans }}{% endblock %}

{% block body %}
    <div>
        <h2>{{ 'front.workout.title'|trans }}</h2>
        <div id="workout" data-workout-id="{{ workoutId }}"></div>
    </div>
{% endblock %}